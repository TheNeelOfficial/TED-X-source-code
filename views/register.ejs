    <script>
        $(document).ready(function() {

            $("#btn1").click(function() {
                $("li>img").filter("#2014").css("display","inherit");
            });

            $("#btn2").click(function() {
                $("li>img").filter("#2013").css("display","inherit");
            });


            $("li>img").filter("#2014").css("display","none");
            $("li>img").filter("#2013").css("display","none");
        });


    </script>
</head>
<body>

    <!-- HEADER -->

    <div class="container">
        <header class="xheader">
            <a href="/"><img class="xlogo" src="/img/logo_web.png"/></a>
            <p class="box1des pull-right" style="float: right;font-size: 1em;font-variant: normal;color: black;">Join us for the next session on <span class="xfont" style="font-size: 1.2em;color: black !important;">APRIL 1<sup>st</sup></span> 2017</p>
        </header>

        <ul class="nav nav-pills xnav">
            <li class="xnave"><a href="/">Home</a></li>
            <li role="presentation" class="dropdown xnave">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#" aria-haspopup="true" aria-expanded="false">Speakers <span class="caret"></span>
                </a>
                <ul class="dropdown-menu">
                <li class="nave"><a href="/2017.ejs">2017</a></li>
                <li class="nave"><a href="/2016">2016</a></li>
